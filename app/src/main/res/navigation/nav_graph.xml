<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/navfragmentdashboard">
    <fragment
        android:id="@+id/navfragmentdashboard"
        android:name="com.example.microsftlogin.TabsFragment.Dashboard"
        android:label="Dashboard"
        tools:layout="@layout/fragment_dashboard">
        <action
            android:id="@+id/action_dashboard3_to_todolistFragment"
            app:destination="@id/todolistFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:popUpTo="@id/nav_graph"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_dashboard3_to_editProfile"
            app:destination="@id/editProfileFragment" />
        <action
            android:id="@+id/action_dashboard3_to_calendarFragment"
            app:destination="@id/calendarFragment"
            app:enterAnim="@anim/nav_default_enter_anim" />
        <action
            android:id="@+id/action_navfragmentdashboard_to_aboutUserFragment"
            app:destination="@id/aboutUserFragment"
            app:launchSingleTop="true"
            app:popUpTo="@id/nav_graph"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_navfragmentdashboard_to_userExperiences"
            app:destination="@id/userExperiences"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_navfragmentdashboard_to_addUserExperience"
            app:destination="@id/addUserExperience" />
        <action
            android:id="@+id/action_navfragmentdashboard_to_userEducation"
            app:destination="@id/userEducation"
            app:launchSingleTop="true"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_navfragmentdashboard_to_userSkills"
            app:destination="@id/userSkills"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_navfragmentdashboard_to_userPersonalProjects"
            app:destination="@id/userPersonalProjects" />
        <action
            android:id="@+id/action_navfragmentdashboard_to_userAchievements"
            app:destination="@id/userAchievements" />
    </fragment>
    <fragment
        android:id="@+id/todolistFragment"
        android:name="com.example.microsftlogin.dashboardsActivities.TodolistFragment"
        android:label="TodoList"
        tools:layout="@layout/fragment_todolist">
        <action
            android:id="@+id/action_todolistFragment_to_addTodoFragment"
            app:destination="@id/addTodoFragment"
            app:enterAnim="@anim/nav_default_enter_anim" />
    </fragment>
    <fragment
        android:id="@+id/editProfileFragment"
        android:name="com.example.microsftlogin.dashboardsActivities.EditProfile"
        android:label="Edit Profile"
        tools:layout="@layout/activity_edit_profile" />
    <fragment
        android:id="@+id/calendarFragment"
        android:name="com.example.microsftlogin.dashboardsActivities.CalendarFragment"
        android:label="Calendar"
        tools:layout="@layout/fragment_calendar" />
    <fragment
        android:id="@+id/addTodoFragment"
        android:name="com.example.microsftlogin.dashboardsActivities.AddTodoFragment"
        android:label="Add Todo"
        tools:layout="@layout/fragment_add_todo">
        <action
            android:id="@+id/action_addTodoFragment_to_todolistFragment"
            app:destination="@id/todolistFragment"
            app:popUpTo="@id/todolistFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/editTodoFragment"
        android:name="com.example.microsftlogin.dashboardsActivities.EditTodoFragment"
        android:label="EditTodoFragment" >
        <action
            android:id="@+id/action_editTodoFragment_to_todolistFragment"
            app:destination="@id/todolistFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_editTodoFragment_to_newActivity"
            app:destination="@id/newActivity"
            app:popUpToInclusive="true" />
    </fragment>
    <activity
        android:id="@+id/newActivity"
        android:name="com.example.microsftlogin.NewActivity"
        android:label="activity_new"
        tools:layout="@layout/activity_new" />
    <fragment
        android:id="@+id/aboutUserFragment"
        android:name="com.example.microsftlogin.EditCvFragements.AboutUserFragment"
        android:label="AboutYou"
        tools:layout="@layout/fragment_about_user" />
    <fragment
        android:id="@+id/userExperiences"
        android:name="com.example.microsftlogin.EditCvFragements.UserExperiences"
        android:label="Experiences" />
    <fragment
        android:id="@+id/addUserExperience"
        android:name="com.example.microsftlogin.EditCvFragements.AddUserExperience"
        android:label="AddExperience" />
    <fragment
        android:id="@+id/userEducation"
        android:name="com.example.microsftlogin.EditCvFragements.UserEducations"
        android:label="Education">
        <action
            android:id="@+id/action_userEducation_to_addUserEducation"
            app:destination="@id/addUserEducation"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/addUserEducation"
        android:name="com.example.microsftlogin.EditCvFragements.AddUserEducation"
        android:label="AddEducation" />
    <fragment
        android:id="@+id/userSkills"
        android:name="com.example.microsftlogin.EditCvFragements.UserSkills"
        android:label="Skills" />
    <fragment
        android:id="@+id/userPersonalProjects"
        android:name="com.example.microsftlogin.EditCvFragements.UserPersonalProjects"
        android:label="PersonalProjects" >
        <action
            android:id="@+id/action_userPersonalProjects_to_addUserPersonalProjects"
            app:destination="@id/addUserPersonalProjects"
            app:launchSingleTop="true"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/addUserPersonalProjects"
        android:name="com.example.microsftlogin.EditCvFragements.AddUserPersonalProjects"
        android:label="AddProjects" />
    <fragment
        android:id="@+id/userAchievements"
        android:name="com.example.microsftlogin.EditCvFragements.UserAchievements"
        android:label="Achievements" />
</navigation>